<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.mutia.rentalio.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_bookingActivity"
            app:destination="@id/bookingActivity" />
        <action
            android:id="@+id/action_homeFragment2_to_bookingFragment"
            app:destination="@id/bookingFragment" />

    </fragment>
    <fragment
        android:id="@+id/helpFragment"
        android:name="com.mutia.rentalio.fragment.HelpFragment"
        android:label="fragment_help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.mutia.rentalio.fragment.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.mutia.rentalio.fragment.AccountFragment"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" />
    <activity
        android:id="@+id/bookingActivity"
        android:name="com.mutia.rentalio.BookingActivity"
        android:label="activity_booking"
        tools:layout="@layout/activity_booking" />
    <fragment
        android:id="@+id/bookingFragment"
        android:name="com.mutia.rentalio.fragment.BookingFragment"
        android:label="fragment_booking"
        tools:layout="@layout/fragment_booking" />
</navigation>